alias ..='cd ..'
alias bat='bat --theme="base16"'
alias cat=bat
alias cp='cp -i'
alias dotgit='/usr/bin/env git --git-dir=$HOME/.dotgit --work-tree=$HOME'
alias exa='exa --icons'
alias gd='git diff'
alias gl='git log --all --decorate --oneline --graph'
alias gs='git status'
alias k=kubectl
alias la='exa --all'
alias lg=lazygit
alias ll='exa --all --long --octal-permissions --git'
alias ls='exa'
alias mv='mv -i'
alias psgrep='pgrep -fl'
alias rm='rm -i'
alias terraform=terraform-v0.12.29
alias tf=terraform-v0.12.29
alias tmux='tmux -2'
alias tm='tmux -2 new-session -A -s 0'
alias uuidgen='uuidgen | tr "[:upper:]" "[:lower:]"'
alias urldecode='python3 -c "import sys, urllib.parse as ul; print(ul.unquote_plus(sys.argv[1]))"'
alias urlencode='python3 -c "import sys, urllib.parse as ul; print (ul.quote_plus(sys.argv[1]))"'
alias vim='TERM=xterm-kitty lvim'
